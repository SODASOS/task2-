#include <iostream>

using namespace std;

int main() {
    int k, l, m;

    //  Запрос ввода чисел
    cout << Введите три целых числа (k l m): ";
    cin >> k >> l >> m;

    //  Проверка на кратность для каждой пары
    cout << "Кратные пары: ";

    if (k != 0 && l % k == 0) {
        cout << "(" << k << ", " << l << ") ";
    }
    if (k != 0 && m % k == 0) {
        cout << "(" << k << ", " << m << ") ";
    }
    if (l != 0 && m % l == 0) {
        cout << "(" << l << ", " << m << ") ";
    }
    if(l != 0 && k % l == 0){
        cout << "(" << l << ", " << k << ") ";
    }
    if (m != 0 && k % m == 0) {
        cout << "(" << m << ", " << k << ") ";
    }
    if (m != 0 && l % m == 0) {
        cout << "(" << m << ", " << l << ") ";
    }

    if(k !=0 && l!=0 && m!=0){
         if (k % l == 0) {
        cout << "(" << k << ", " << l << ") ";
    }
    if (l % k == 0) {
        cout << "(" << l << ", " << k << ") ";
    }


    if (k % m == 0) {
        cout << "(" << k << ", " << m << ") ";
    }
    if (m % k == 0) {
        cout << "(" << m << ", " << k << ") ";
    }


    if (l % m == 0) {
        cout << "(" << l << ", " << m << ") ";
    }
    if (m % l == 0) {
        cout << "(" << m << ", " << l << ") ";
    }
    }



  cout << endl;  // Добавляем для красоты 

    return 0;
}
