#include <iostream>
using namespace std;

int main() {
    double a, b, c;
    
    // Ввод трех чисел
    cout << "Введите длины сторон треугольника a, b, c: ";
    cin >> a >> b >> c;

    // Проверка неравенства треугольника
    if ((a + b > c) && (a + c > b) && (b + c > a)) {
        // Все три условия выполнены, значит можно построить треугольник

        // Определение типа треугольника
        if (a == b && b == c) {
            cout << "Треугольник равносторонний." << endl;
        } else if (a == b || a == c || b == c) {
            cout << "Треугольник равнобедренный." << endl;
        } else {
            cout << "Треугольник разносторонний." << endl;
        }
    } else {
        cout << "Из данных сторон нельзя составить треугольник." << endl;
    }

    return 0;
}
